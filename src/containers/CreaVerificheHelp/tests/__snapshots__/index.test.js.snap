    <CreaVerificheHelp__Img
      alt="Selezione esercizi"
      responsive={true}
      src="/basename/help_matematica_selezione.png"
    />
    <Paragraph
      theme={
        Object {
          "brand": "#00BBEF",
          "darken": "#1588AA",
        }
      }
